---
import { itemClasses } from "@/content/config";

export async function getStaticPaths() {
  const res: { params: { itemClass: string } }[] = itemClasses.reduce((paths, itemClass) => {
    return [ ...paths, { params: { itemClass } } ];
  }, [] as { params: { itemClass: string } }[]);
  return res;
}
const { itemClass } = Astro.params;
const redirectPath = `/item-classes/${itemClass}/p/1`;

Astro.redirect(redirectPath);
